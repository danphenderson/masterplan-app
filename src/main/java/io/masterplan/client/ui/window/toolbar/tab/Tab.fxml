<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import io.masterplan.client.ui.util.icon.Icon?>

<!-- Styled toolbar Tab with Icon -->
<fx:root type="javafx.scene.layout.HBox" stylesheets="@Tab.css" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <Button fx:id="button" HBox.hgrow="ALWAYS">
        <graphic>
            <Icon fx:id="icon"/>
        </graphic>
    </Button>
</fx:root>
